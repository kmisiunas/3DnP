<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="300.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.misiunas.np.gui.manual.ManualController">
   <children>
      <AnchorPane>
         <children>
            <Label layoutX="14.0" layoutY="11.0" text="Processor">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <ProgressIndicator fx:id="progressIndicator" focusTraversable="false" layoutX="650.0" layoutY="7.0" prefHeight="36.0" prefWidth="36.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="7.0" />
            <Button fx:id="killProcessorButton" layoutX="530.0" layoutY="12.0" mnemonicParsing="false" onAction="#killProcessorAction" style="-fx-base: Red;" text="Kill Process" textAlignment="CENTER" underline="true" />
         </children>
      </AnchorPane>
      <Separator prefWidth="200.0">
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="4.0" />
         </VBox.margin></Separator>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <GridPane>
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="70.0" prefWidth="100.0" />
                      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints vgrow="SOMETIMES" />
                        <RowConstraints vgrow="SOMETIMES" />
                        <RowConstraints minHeight="-Infinity" vgrow="SOMETIMES" />
                        <RowConstraints prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Approach distance fraction">
                           <GridPane.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </GridPane.margin>
                        </Label>
                        <TextField fx:id="approachDistanceField" GridPane.columnIndex="1">
                           <GridPane.margin>
                              <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                           </GridPane.margin>
                        </TextField>
                        <Label text="Approach speed (um/iteration)" GridPane.rowIndex="1">
                           <GridPane.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </GridPane.margin>
                        </Label>
                        <Label text="Bulk coductivity check interval (s)" GridPane.rowIndex="2">
                           <GridPane.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </GridPane.margin>
                        </Label>
                        <TextField fx:id="approachSpeedField" GridPane.columnIndex="1" GridPane.rowIndex="1">
                           <GridPane.margin>
                              <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                           </GridPane.margin>
                        </TextField>
                        <TextField fx:id="conductivityCheckIntervalField" GridPane.columnIndex="1" GridPane.rowIndex="2">
                           <GridPane.margin>
                              <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                           </GridPane.margin>
                        </TextField>
                        <Label text="Pulse size (" GridPane.rowIndex="4">
                           <GridPane.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </GridPane.margin>
                        </Label>
                        <Label text="Use AC " GridPane.rowIndex="3">
                           <GridPane.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </GridPane.margin>
                        </Label>
                        <CheckBox fx:id="useACToggle" accessibleHelp="Use AC current for monitoring height" disable="true" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <Label text="Deposition Mode" GridPane.rowIndex="5">
                           <GridPane.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </GridPane.margin>
                        </Label>
                        <CheckBox fx:id="depositionModeToggle" accessibleHelp="Use AC current for monitoring height" mnemonicParsing="false" onAction="#depositionModeAction" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                        <TextField fx:id="pulseSizeField" GridPane.columnIndex="1" GridPane.rowIndex="4">
                           <GridPane.margin>
                              <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                           </GridPane.margin>
                        </TextField>
                     </children>
                  </GridPane>
               </children>
               <GridPane.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </GridPane.margin>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="5.0" GridPane.columnIndex="1">
               <children>
                  <Button fx:id="approachSurfaceButton" mnemonicParsing="false" onAction="#approachSurfaceAction" prefWidth="200.0" text="Approach Surface">
                     <VBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="findAndFollowSurfaceButton" mnemonicParsing="false" onAction="#findAndFollowSurfaceAction" prefWidth="200.0" text="Find and Follow Surface">
                     <VBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </VBox.margin></Button>
                  <Button mnemonicParsing="false" onAction="#setStandardDACSettingsAction" prefWidth="200.0" text="Set Standard DAC settings">
                     <VBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#depositPulseAction" prefWidth="200.0" text="Deposit Pulse">
                     <VBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#zeroCurrentAction" prefWidth="200.0" text="Zero Current">
                     <VBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </VBox>
         </children>
      </GridPane>
   </children>
</VBox>
